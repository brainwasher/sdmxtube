        {
            "name": "PowerShell: Launch Script",
            "type": "PowerShell",
            "request": "launch",
            "script": "${workspaceFolder}/sdmx2gv/runConversion.ps1",
            "cwd": "${workspaceFolder}",
            "args": [   "-Verbose" 
